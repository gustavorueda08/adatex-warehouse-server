{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "barcode": {
      "type": "uid"
    },
    "alternativeBarcode": {
      "type": "string"
    },
    "originalQuantity": {
      "type": "decimal",
      "required": true
    },
    "currentQuantity": {
      "type": "decimal",
      "required": true
    },
    "unit": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "kg",
        "m",
        "piece",
        "unit"
      ]
    },
    "state": {
      "type": "enumeration",
      "default": "available",
      "enum": [
        "available",
        "reserved",
        "sold",
        "dropped"
      ]
    },
    "lotNumber": {
      "type": "string"
    },
    "receiptDate": {
      "type": "datetime"
    },
    "lastMovementDate": {
      "type": "datetime"
    },
    "isPartition": {
      "type": "boolean",
      "default": false
    },
    "partitionNumber": {
      "type": "integer"
    },
    "qualityStatus": {
      "type": "enumeration",
      "default": "approved",
      "enum": [
        "pending",
        "approved",
        "rejected",
        "conditional"
      ]
    },
    "qualityNotes": {
      "type": "text"
    },
    "itemNumber": {
      "type": "string"
    },
    "qualityCheckDate": {
      "type": "datetime"
    },
    "cost": {
      "type": "decimal"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "items"
    },
    "warehouse": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::warehouse.warehouse",
      "inversedBy": "items"
    },
    "sourceOrder": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "sourceItems"
    },
    "orderProducts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order-product.order-product",
      "inversedBy": "items"
    },
    "childItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "parentItem"
    },
    "parentItem": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::item.item",
      "inversedBy": "childItems"
    },
    "transformedFromItem": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::item.item",
      "inversedBy": "transformedItems"
    },
    "transformedItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "transformedFromItem"
    },
    "qualityCheckedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "qualityCheckedItems"
    },
    "lastModifiedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "modifiedItems"
    },
    "movements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory-movement.inventory-movement",
      "mappedBy": "item"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "inversedBy": "items"
    },
    "cbm": {
      "type": "decimal",
      "default": 0
    },
    "weight": {
      "type": "decimal",
      "default": 1
    },
    "isInvoiced": {
      "type": "boolean",
      "default": false
    },
    "invoicedDate": {
      "type": "datetime"
    }
  }
}

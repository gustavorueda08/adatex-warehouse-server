{
  "kind": "collectionType",
  "collectionName": "prices",
  "info": {
    "singularName": "price",
    "pluralName": "prices",
    "displayName": "Price"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "unitPrice": {
      "type": "decimal",
      "required": true
    },
    "ivaIncluded": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "priceList"
    },
    "supplier": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::supplier.supplier",
      "inversedBy": "prices"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "prices"
    },
    "invoicePercentage": {
      "type": "decimal",
      "required": false,
      "default": 100
    }
  }
}

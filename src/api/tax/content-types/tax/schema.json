{
  "kind": "collectionType",
  "collectionName": "taxes",
  "info": {
    "singularName": "tax",
    "pluralName": "taxes",
    "displayName": "Tax"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "default": "percentage",
      "enum": [
        "percentage",
        "fixed"
      ]
    },
    "applicationType": {
      "type": "enumeration",
      "default": "subtotal",
      "enum": [
        "product",
        "subtotal",
        "auto"
      ]
    },
    "siigoCode": {
      "type": "uid"
    },
    "tresholdContidion": {
      "type": "enumeration",
      "enum": [
        "greaterThan",
        "lessThan",
        "greaterThanOrEqualTo",
        "lessThanOrEqualTo"
      ]
    },
    "treshold": {
      "type": "decimal",
      "default": 0
    },
    "customers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::customer.customer",
      "mappedBy": "taxes"
    },
    "use": {
      "type": "enumeration",
      "default": "decrement",
      "enum": [
        "increment",
        "decrement"
      ]
    }
  }
}

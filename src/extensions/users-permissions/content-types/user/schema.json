{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "generatedOrders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "generatedBy"
    },
    "editedOrders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "editedBy"
    },
    "cancelledOrders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "cancelledBy"
    },
    "qualityCheckedItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "qualityCheckedBy"
    },
    "modifiedItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "lastModifiedBy"
    },
    "movements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory-movement.inventory-movement",
      "mappedBy": "user"
    },
    "usedVirtualBarcodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::barcode-mapping.barcode-mapping",
      "mappedBy": "usedBy"
    },
    "generatedVirtualBarcodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::barcode-mapping.barcode-mapping",
      "mappedBy": "generatedBy"
    },
    "type": {
      "type": "enumeration",
      "default": "admin",
      "enum": [
        "admin",
        "warehouseKeeper",
        "printer",
        "seller"
      ]
    },
    "name": {
      "type": "string"
    },
    "seller": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::seller.seller",
      "inversedBy": "user"
    }
  }
}
